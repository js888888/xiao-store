<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiao.store.mapper.XiaoProductMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xiao.store.entity.XiaoProduct">
        <result column="id" property="id" />
        <result column="CREATED_TIME" property="createdTime" />
        <result column="UPDATED_TIME" property="updatedTime" />
        <result column="product_name" property="productName" />
        <result column="product_photo_url" property="productPhotoUrl" />
        <result column="product_description" property="productDescription" />
        <result column="product_price" property="productPrice" />
        <result column="product_specifications" property="productSpecifications" />
        <result column="untitled" property="untitled" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        CREATED_TIME, UPDATED_TIME, product_name, product_photo_url, product_description, product_price, product_specifications, untitled
    </sql>

</mapper>
